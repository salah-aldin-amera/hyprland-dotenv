{
    "extractor": {
        "archive": "postgresql://postgres:password@localhost:5432/gallery_dl",
        "archive-mode": "file",
        "tiktok": {
            "filename": "{date:%Y-%m-%d %H-%M-%S} - {title:.80} [{id}].{extension}",
            "sleep-request": [1, 15]
        },
        "instagram": {
            "filename": "{date:%Y-%m-%d %H-%M-%S}",
            "sleep-request": [1, 15],
            "include": "all"
        },
        "youtube": {
            "filename": "{upload_date:%Y-%m-%d %H-%M-%S} - {title:.80} [{id}].{extention}",
            "sleep-request": [1, 5]
        }
    },
    "cookies-from-browser": ["opera"]
}
